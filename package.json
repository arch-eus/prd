{
  "name": "task-manager",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "clean-dev": "rm -rf .svelte-kit && rm -rf node_modules/.vite && vite dev --force",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal",
    "collaboration-server": "node collaboration-server.js",
    "lint": "eslint --ext .js,.ts,.svelte .",
    "lint:fix": "eslint --ext .js,.ts,.svelte . --fix",
    "format": "prettier --write 'src/**/*.{js,ts,svelte,css}'",
    "analyze": "vite build && open dist/stats.html",
    "deps:check": "npx depcheck",
    "deps:update": "npx npm-check-updates -u"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.4.3",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^8.30.1",
    "@vite-pwa/assets-generator": "^1.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.24.0",
    "eslint-plugin-svelte": "^3.5.1",
    "npm-check-updates": "^17.1.18",
    "postcss": "^8.4.33",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.3",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^1.2.2",
    "workbox-window": "^7.3.0"
  },
  "dependencies": {
    "@stablelib/base64": "^2.0.1",
    "@syncedstore/core": "^0.6.0",
    "@syncedstore/svelte": "^0.6.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "date-fns": "^3.3.1",
    "idb-keyval": "^6.2.1",
    "lucide-svelte": "^0.314.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.1",
    "y-indexeddb": "^9.0.12",
    "y-websocket": "^3.0.0"
  }
}
